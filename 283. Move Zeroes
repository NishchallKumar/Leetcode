//First Code
class Solution {
    public void moveZeroes(int[] nums) {
        ArrayList<Integer>ans = new ArrayList<Integer>();
        

        for(int i = 0; i< nums.length; i++){
            if(nums[i] != 0){
                ans.add(nums[i]);
            }
        }
        int idx = 0 ;
        for(int i : ans){
            nums[idx] = i;
            idx++;
        }
        while(idx < nums.length){
            nums[idx++] = 0;
        }
        System.out.println(ans);
        
    }
}

//Second Code
class Solution {
    public void moveZeroes(int[] nums) {
       int arr[] = nums;
       int j = 0;
        for(int i=0; i<arr.length; i++){
            if (arr[i] != 0){
                nums[j] = arr[i];
                j++;
            }
        }
        for(;j<nums.length;j++){
            nums[j] = 0;
        }
    }
}
